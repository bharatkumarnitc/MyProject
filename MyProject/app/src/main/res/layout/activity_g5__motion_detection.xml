<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/G5_ll"
    tools:context="online.nitcalicut.myproject.Hardware.G5_MotionDetection">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="15dp"
        android:text="Motion Detection "
        android:textColor="@color/md_black_1000"
        android:textSize="18sp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/G5_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/mymarginLeft"
        android:layout_marginTop="@dimen/mymarginTop"
        android:textColor="@color/md_black_1000"
        android:textSize="@dimen/mytextsize" />

</LinearLayout>
